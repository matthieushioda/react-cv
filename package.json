{
	"name": "react-cv",
	"version": "1.0.0",
	"private": true,
	"description": "React Curriculum Vitae",
	"homepage": "https://matthieushioda.github.io/react-cv",
	"repository": {
		"type": "git",
		"url": "https://github.com/matthieushioda/react-cv.git"
	},
	"author": "Matthieu Shioda",
	"scripts": {
		"pw": "npx prettier --write .",
		"clean-all": "rm -rf package-lock.json && rm -rf ./build && rm -rf ./node_modules",
		"scan-i18n": "i18next-scanner --config i18next-scanner.config.js",
		"start": "npm run scan-i18n && react-scripts start",
		"build": "npm run scan-i18n && react-scripts build",
		"test": "npm run scan-i18n && react-scripts test",
		"eject": "react-scripts eject",
		"predeploy": "npm run build",
		"deploy": "gh-pages -d build"
	},
	"dependencies": {
		"@emotion/react": "11.10.5",
		"@emotion/styled": "11.10.5",
		"@mui/icons-material": "5.11.0",
		"@mui/material": "5.11.1",
		"@reduxjs/toolkit": "1.9.1",
		"browser-fs-access": "0.31.1",
		"i18next": "22.4.6",
		"i18next-xhr-backend": "3.2.2",
		"lodash": "4.17.21",
		"react": "18.2.0",
		"react-dom": "18.2.0",
		"react-error-boundary": "3.1.4",
		"react-i18next": "12.1.1",
		"react-redux": "8.0.5",
		"react-router-dom": "6.6.0",
		"react-scripts": "5.0.1",
		"typescript": "4.9.4"
	},
	"devDependencies": {
		"@types/lodash": "^4.14.191",
		"@types/node": "18.11.17",
		"@types/react": "18.0.26",
		"@types/react-dom": "18.0.10",
		"@types/react-redux": "7.1.24",
		"@types/react-router-dom": "5.3.3",
		"@typescript-eslint/eslint-plugin": "5.47.0",
		"@typescript-eslint/parser": "5.47.0",
		"eslint": "8.30.0",
		"eslint-config-prettier": "8.5.0",
		"eslint-plugin-prettier": "4.2.1",
		"eslint-plugin-react": "7.31.11",
		"eslint-plugin-react-hooks": "4.6.0",
		"gh-pages": "4.0.0",
		"husky": "8.0.2",
		"i18next-scanner": "4.1.0",
		"i18next-scanner-typescript": "1.0.6",
		"lint-staged": "13.1.0",
		"prettier": "2.8.1",
		"prettier-eslint": "15.0.1"
	},
	"build": {
		"engines": {
			"npm": "8.5",
			"node": "16"
		}
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"**/*": "prettier --write --ignore-unknown"
	},
	"eslintConfig": {
		"extends": [
			"react-app",
			"react-app/jest"
		]
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	}
}
